{
  "compilerOptions": {
    "strict": true,
    "target": "es2018",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": ["node"],
    "paths": {
      "#app": ["./node_modules/@nuxt/bridge/dist/runtime"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
